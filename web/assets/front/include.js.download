        $.when.apply($,
        	$.map(urls, function(url){
                if (nocache) url += '?_ts=' + new Date().getTime(); // refresh? 
                return $.get(url, function(){                    
                	$('<link>', {rel:'stylesheet', type:'text/css', 'href':url}).appendTo('head');                    
                });
            })
        	).then(function(){
        		if (typeof callback=='function') callback();
        	});
        }
    });
	

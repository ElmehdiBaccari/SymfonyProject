		}
		
		var $form = $container.find('[data-optionform]');
		
		$form.find('input,button').on('click',function(){
			$('body').addClass('loading')
			$form.submit();
		})
		
		
	}
	
	
	return{
		init:init,
		pager:pager
	};
	

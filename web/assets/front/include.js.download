            var data={};
            data.action='author-love';
            data.author_id = author_id
            data.yesno=yesno?'1':'0';
            
            return app.ajax(data);


        }

        var $el = $container.find('[data-love-app]');
        $el.css('visibility', 'visible');
        var data=$el.data('love-app')
        data.over=false;
        data.loading=false;

        var vueapp=new Vue({
            el:$el[0],
            delimiters:['[[',']]'],
            data:data,
            methods:{
                update:function(yesno){
                    var comp = this;
                    comp.loading=true;
                    love(yesno).then(function(){
                        comp.loading=false;
                    })
                },
                changeLove:function(){
                    if(app.data.connectedMode==0){
                        return app.views.authent.openLoginForm();
                    }
                    if(this.loading){
                        return;
                    }
                    if(true == this.i_do){
                        this.i_do = false;
                        this.count--;
                        this.update(false)
                        
                    }else{
                        this.i_do = true;
                        this.count++;
                        this.update(true)
                        
                    }
                }
            }
        });

    }


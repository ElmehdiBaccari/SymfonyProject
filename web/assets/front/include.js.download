			var elem=e.target;
			
			if(elem.getAttribute('data-lazy')=='youtubeiframe'){
				var $elem=$(elem);
				$elem.html(  $elem.find('script').html() );
				
				
				
				
				
				window.youtubeApiPromise.then(function(){
					var $iframe=$elem.find('iframe');
					var elem_id = $iframe.attr('id')
					
					var player=null;
					
					player = new YT.Player(elem_id,{
						
						events: {
